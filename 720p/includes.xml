				<include>VisibleFadeEffect</include>
			</control>
			<control type="image">
				<width>35</width>
				<height>35</height>
				<aspectratio>keep</aspectratio>
				<texture>icon_paused.png</texture>
				<visible>Player.Paused</visible>
