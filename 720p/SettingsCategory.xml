<window id="12">
	<defaultcontrol>3</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<include>CommonBackground</include>
		<control type="group">
			<include>ViewVisibleEffect</include>
			<control type="grouplist" id="5">
				<description>control area</description>
				<posx>340</posx>
				<posy>70</posy>
				<width>880</width>
				<height>570</height>
				<itemgap>0</itemgap>
				<pagecontrol>90</pagecontrol>
				<onleft>3</onleft>
				<onright>90</onright>
				<onup>5</onup>
				<ondown>5</ondown>
			</control>
			<control type="scrollbar" id="90">
				<posx>1225</posx>
				<posy>70</posy>
				<width>17</width>
				<height>570</height>
				<texturesliderbackground border="2,2,2,2">scrollbar_bar_back.png</texturesliderbackground>
				<texturesliderbar border="2,16,2,16">scrollbar_bar.png</texturesliderbar>
				<texturesliderbarfocus border="2,16,2,16">scrollbar_bar_focus.png</texturesliderbarfocus>
				<textureslidernib>scrollbar_nib.png</textureslidernib>
				<textureslidernibfocus>scrollbar_nib.png</textureslidernibfocus>
				<onleft>5</onleft>
				<onright>3</onright>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
			</control>
		</control>
		<control type="group">
			<animation type="WindowOpen" reversible="false">
				<effect type="zoom" start="80" end="100" center="640,360" easing="out" tween="back" time="300" />
				<effect type="fade" start="0" end="100" time="300" />
			</animation>
			<animation type="WindowClose" reversible="false">
				<effect type="zoom" start="100" end="80" center="640,360" easing="in" tween="back" time="300" />
				<effect type="fade" start="100" end="0" time="300" />
			</animation>
			<posx>40</posx>
			<posy>45</posy>
			<control type="image">
				<description>shadow border</description>
				<posx>-8</posx>
				<posy>17</posy>
				<width>306</width>
				<height>431</height>
				<texture border="10">border-shadow.png</texture>
			</control>
			<control type="image">
				<description>Down Arrow</description>
				<posx>30</posx>
				<posy>0</posy>
				<width>40</width>
				<height>30</height>
				<texture>arrow-big-up.png</texture>
			</control>
			<control type="image">
				<description>background image</description>
				<posx>0</posx>
				<posy>25</posy>
				<width>290</width>
				<height>415</height>
				<texture border="8">SubBack.png</texture>
			</control>
			<control type="image">
				<description>Overlay image</description>
				<posx>0</posx>
				<posy>25</posy>
				<width>200</width>
				<height>310</height>
				<texture>GlassOverlay.png</texture>
			</control>
			<control type="image">
				<posx>0</posx>
				<posy>25</posy>
				<width>290</width>
				<height>40</height>
				<texture>HorizontalGlow.png</texture>
			</control>
			<control type="grouplist" id="3">
				<description>button area</description>
				<posx>5</posx>
				<posy>35</posy>
				<width>280</width>
				<height>405</height>
				<itemgap>0</itemgap>
				<onleft>5</onleft>
				<onright>5</onright>
				<onup>3</onup>
				<ondown>3</ondown>
			</control>
		</control>
		<control type="button" id="10">
			<description>Default Category Button</description>
			<include>ButtonCommonValues</include>
		</control>
		<control type="button" id="7">
			<description>Default Button</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>45</height>
			<font>font12</font>
			<textcolor>white</textcolor>
			<focusedcolor>black</focusedcolor>
			<texturefocus border="5">ThumbFO.png</texturefocus>
			<texturenofocus border="5">ListNF.png</texturenofocus>
		</control>
		<control type="radiobutton" id="8">
			<description>Default RadioButton</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>45</height>
			<font>font12</font>
			<textcolor>white</textcolor>
			<focusedcolor>black</focusedcolor>
			<texturefocus border="5">ThumbFO.png</texturefocus>
			<texturenofocus border="5">ListNF.png</texturenofocus>
		</control>
		<control type="spincontrolex" id="9">
			<description>Default SpinControlex</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>45</height>
			<font>font12</font>
			<textcolor>white</textcolor>
			<focusedcolor>black</focusedcolor>
			<texturefocus border="5">ThumbFO.png</texturefocus>
			<texturenofocus border="5">ListNF.png</texturenofocus>
			<aligny>center</aligny>
			<reverse>yes</reverse>
		</control>
		<control type="image" id="11">
			<description>separator image</description>
			<height>4</height>
			<texture>-</texture>
		</control>
		<include>WindowHeader</include>
		<control type="image">
			<posx>20</posx>
			<posy>5</posy>
			<width>40</width>
			<height>35</height>
			<aspectratio>keep</aspectratio>
			<texture>icon_system.png</texture>
			<include>Window_OpenClose_Animation</include>
		</control>
		<control type="label">
			<description>Header label</description>
			<posx>70</posx>
			<posy>5</posy>
			<width>700</width>
			<height>35</height>
			<align>left</align>
			<aligny>center</aligny>
			<font>font13</font>
			<textcolor>white</textcolor>
			<label>$LOCALIZE[5] - $INFO[Control.GetLabel(2)]</label>
			<include>Window_OpenClose_Animation</include>
		</control>
		<control type="label" id="2">
			<description>Fake Label so we can pass it value down to the one above</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>0</width>
			<height>0</height>
			<font>-</font>
			<visible>false</visible>
		</control>
		<include>Common_RSS_Feed</include>
		<include>BehindDialogFadeOut</include>
	</controls>
</window>
